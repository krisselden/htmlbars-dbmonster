<table class="table table-striped latest-data">
  <tbody>
    {{#each dbs as |db|}}
      <tr>
        <td class="dbname">{{db.name}}</td>
        <td class="query-count">
          <span class="{{db.countClassName}}">{{db.queries.length}}</span>
        </td>
        {{#each db.topFiveQueries as |query|}}
          <td class="Query {{query.className}}">
            {{query.elapsed}}
            <div class="popover left">
              <div class="popover-content">{{query.query}}</div>
              <div class="arrow"></div>
            </div>
          </td>
        {{/each}}
      </tr>
    {{/each}}
  </tbody>
</table>
